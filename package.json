{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run lite\" \"npm run watch:scss\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin":"img/* -o dist/img"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.3.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-beta1",
    "bootstrap-social": "^5.1.1",
    "copyfiles": "^2.4.1",
    "imagemin-cli": "^6.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "less": "^4.1.0",
    "popper.js": "^1.16.1",
    "sass": "^1.32.5",
    "scss": "^0.2.4"
  }
}
